	cam0_vcamio-supply = <&mt6358_vcamio_reg>;
	cam0_vcamd-supply = <&mt6358_vcamio_reg>;
	cam0_vcamaf-supply = <&mt6358_vcamio_reg>;

	/* front hi1634*/
	cam1_vcama-supply = <&mt6358_vcamio_reg>;
	cam1_vcamio-supply = <&mt6358_vcamio_reg>;
	cam1_vcamd-supply = <&mt6358_vcamio_reg>;

	/* main2 s5k4h7*/
