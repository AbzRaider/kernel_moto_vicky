&thermal_zones{
		cam_ntc: cam_ntc {
			polling-delay = <0>; /* milliseconds */
			polling-delay-passive = <0>; /* milliseconds */
			thermal-sensors = <&tboard_thermistor3>;
		};
		chg_ntc: chg_ntc {
			polling-delay = <0>; /* milliseconds */
			polling-delay-passive = <0>; /* milliseconds */
			thermal-sensors = <&tboard_thermistor4>;
		};
		quiet_thermal: quiet_thermal {
		        polling-delay = <5000>; /* milliseconds */
		        polling-delay-passive = <1000>; /* milliseconds */
		        thermal-sensors = <&tboard_thermistor5>;
		        trips {
			    charge_trip0: charge-trip0 {
			    temperature = <40000>;
			    hysteresis = <3000>;
			    type = "passive";
			};
			charge_trip1: charge-trip1 {
			    temperature = <42500>;
			    hysteresis = <3000>;
			    type = "passive";
			};
			charge_trip2: charge-trip2 {
			    temperature = <43500>;
			    hysteresis = <3000>;
			    type = "passive";
			};
			charge_trip3: charge-trip3 {
			    temperature = <44500>;
			    hysteresis = <3000>;
			    type = "passive";
			};
			charge_trip4: charge-trip4 {
			    temperature = <45000>;
			    hysteresis = <3500>;
			    type = "passive";
			};
			charge_trip5: charge-trip5 {
			    temperature = <46000>;
			    hysteresis = <4000>;
			    type = "passive";
			};
		    };

		    cooling-maps {
			charge-cdev0 {
			    trip = <&charge_trip0>;
			    cooling-device = <&charger_cooler 1 1>;
			};
			charge-cdev1 {
			    trip = <&charge_trip1>;
			    cooling-device = <&charger_cooler 2 2>;
			};
			charge-cdev2 {
			    trip = <&charge_trip2>;
			    cooling-device = <&charger_cooler 3 3>;
			};
			charge-cdev3 {
			    trip = <&charge_trip3>;
