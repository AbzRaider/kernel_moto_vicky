&i2c0 {
        status = "okay";
        clock-frequency = <400000>;
        flash_aw36515:aw36515@63 {
        compatible = "mediatek,aw36515";
        reg = <0x63>;
        status = "okay";
        };
};

/* CAMERA EEPROM */
&i2c2 {
	status = "okay";
	clock-frequency = <1000000>;
	#address-cells = <1>;
	size-cells = <0>;

	camera_main_mtk:camera_main@1a {
