/dts-v1/;
/plugin/;

#include "mediatek/mt6879-tesla-common-overlay.dtsi"
#include "mediatek/tesla-connsys-na.dtsi"

#define TESLA_RADIO_NA 1

/* Board match root node */
/ {
	fragment@model {
		target-path = "/";
		__overlay__ {
			mot,model = "tesla";
			mot,board-id = <(TESLA_RADIO_NA << 8 | 0x41) 0xA300>;
		};
	};
};

