/* CAMERA TZ config */
&thermal_zones {
	camera_main: camera_main {
		polling-delay = <1000>; /* milliseconds */
		polling-delay-passive = <0>; /* milliseconds */
		thermal-sensors = <&camera_main_mtk>;
	};
	camera_sub: camera_sub {
		polling-delay = <1000>; /* milliseconds */
		polling-delay-passive = <0>; /* milliseconds */
		thermal-sensors = <&camera_sub_mtk>;
	};
	camera_main_two: camera_main_two {
		polling-delay = <1000>; /* milliseconds */
		polling-delay-passive = <0>; /* milliseconds */
		thermal-sensors = <&camera_main_two_mtk>;
	};
	camera_main_three: camera_main_three {
		polling-delay = <1000>; /* milliseconds */
		polling-delay-passive = <0>; /* milliseconds */
		thermal-sensors = <&camera_main_three_mtk>;
	};
};
/* CAMERA TZ config end */

/* CAMERA Flashlight */
&flashlights_devonf {
	compatible = "mediatek,flashlights_devonf_pwm";
	pinctrl-names = "default",
		"flashlight_gpio_high",
		"flashlight_gpio_low",
		"flashlight_pwm_high",
		"flashlight_pwm_low",
		"flashlight_pwm_en";

	pinctrl-0 = <&flash_default>;
	pinctrl-1 = <&flashlight_gpio_high>;
	pinctrl-2 = <&flashlight_gpio_low>;
	pinctrl-3 = <&flashlight_pwm_high>;
	pinctrl-4 = <&flashlight_pwm_low>;
	pinctrl-5 = <&flashlight_pwm_en>;

	srcclk = <&pericfg_ao_clk>;
	decouple = <1>;
	channel@1 {
		type = <0>;
		ct = <0>;
		part = <0>;
	};
};
/* CAMERA Flashlight end */
